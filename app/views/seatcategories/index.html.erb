<h1>Listing seatcategories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @seatcategories.each do |seatcategory| %>
      <tr>
        <td><%= seatcategory.name %></td>
        <td><%= link_to 'Show', seatcategory %></td>
        <td><%= link_to 'Edit', edit_seatcategory_path(seatcategory) %></td>
        <td><%= link_to 'Destroy', seatcategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seatcategory', new_seatcategory_path %>
