  <body>
    
    <div lass="container">
<div class="row centered">
	<div class="col-lg-8 col-lg-offset-2">

<%= form_for(resource,class: 'form-horizontal', :as => resource_name, :url => session_path(resource_name)) do |f| %>
   <div class="form-group">
    <div class="col-sm-10">
      <h2 for="inputEmail3" class="col-sm-8 col-lg-offset-4 control-label"><strong>Sign in</strong></h2>
    </div>
  </div>
    
   <div class="form-group">
    <div class="col-sm-10">
      <label for="inputEmail3" class="col-sm-2 col-lg-offset-2 control-label"><i class="fa fa-user"></i></label>
      <%= f.email_field :email, :autofocus => true, placeholder: 'Email',class:'col-sm-8', id: 'inputEmail3' %>
    </div>
  </div>
  
    
   <div class="form-group">
    <div class="col-sm-10">
      <label for="inputEmail3" class="col-sm-2 col-lg-offset-2 control-label"><i class="fa fa-cogs"></i></label>
      <%= f.password_field :password, :autofocus => true, placeholder: 'Password',class:'col-sm-8', id: 'inputEmail3' %>
    </div>
  </div>
  
    
  <div class="form-group">
    <div class="col-sm-10">
      <div><%= f.submit "Sign in" ,class:'col-sm-2 col-sm-offset-7'%><BR>
          <div class="col-sm-2 col-sm-offset-7">
          <% if devise_mapping.rememberable? -%>
              <div class:'col-sm-7 col-sm-offset-2'><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
          <% end %>
          </div>
      </div>
    </div>
  </div>



<% end %>

    </div>
  </div><!-- row -->
</div>

    
